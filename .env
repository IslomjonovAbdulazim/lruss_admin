# API Configuration
REACT_APP_API_BASE_URL=https://lrussrubackend-production.up.railway.app

# Admin Credentials (for reference - actual values should be set on backend)
# ADMIN_PHONE=+998901234567
# ADMIN_PASSWORD=your_admin_password

# App Configuration
REACT_APP_NAME=LRUSS Admin
REACT_APP_VERSION=1.0.0

# Development Settings
REACT_APP_ENV=development
GENERATE_SOURCEMAP=true

# Build Settings
REACT_APP_BUILD_DATE=$npm_config_build_date

# Optional: Analytics and Monitoring
# REACT_APP_GOOGLE_ANALYTICS_ID=
# REACT_APP_SENTRY_DSN=

# Optional: Feature Flags
REACT_APP_ENABLE_OFFLINE_MODE=false
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_ANALYTICS=false

# Cache Settings
REACT_APP_CACHE_ENABLED=true
REACT_APP_CACHE_TTL=300000

# Security Settings
REACT_APP_SESSION_TIMEOUT=86400000